-- 4. UTILITY: CREATE IOS-STYLE APP PAGES
local function createBaseApp(title)
    local appPage = Instance.new("ScrollingFrame", phoneFrame)
    appPage.Size = UDim2.new(1, 0, 0.8, 0)
    appPage.Position = UDim2.new(0, 0, 0.1, 0)
    appPage.Visible = false
    appPage.BackgroundColor3 = Color3.fromRGB(240, 240, 240)
    appPage.AutomaticCanvasSize = Enum.AutomaticSize.Y
    appPage.ScrollBarThickness = 0
    appPage.BorderSizePixel = 0
    
    local header = Instance.new("TextLabel", appPage)
    header.Size = UDim2.new(1, 0, 0, 50)
    header.Text = title
    header.BackgroundColor3 = Color3.fromRGB(120, 140, 180)
    header.TextColor3 = Color3.new(1, 1, 1)
    header.Font = Enum.Font.GothamBold
    header.TextSize = 20
    
    local list = Instance.new("UIListLayout", appPage)
    list.Padding = UDim.new(0, 0)
    return appPage
end

local function addListItem(parent, name, code)
    local btn = Instance.new("TextButton", parent)
    btn.Size = UDim2.new(1, 0, 0, 45)
    btn.BackgroundColor3 = Color3.new(1, 1, 1)
    btn.BorderSizePixel = 1
    btn.BorderColor3 = Color3.fromRGB(210, 210, 210)
    btn.Text = "  " .. name
    btn.Font = Enum.Font.GothamMedium
    btn.TextColor3 = Color3.new(0, 0, 0)
    btn.TextSize = 15
    btn.TextXAlignment = Enum.TextXAlignment.Left
    if code then btn.MouseButton1Click:Connect(function() loadstring(code)() end) end
    return btn
end

-- 5. DEFINE INDIVIDUAL APPS
local chatLogs = createBaseApp("Chat Logs")
local hubsApp = createBaseApp("Hubs")
local animsApp = createBaseApp("Animations")

-- App Selection Buttons
local function createAppBtn(name, icon, target)
    local btn = addListItem(appSelection, icon .. " " .. name)
    btn.MouseButton1Click:Connect(function()
        appSelection.Visible = false
        target.Visible = true
    end)
end

