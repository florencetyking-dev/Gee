-- Chat Logs Updater
TextChatService.OnIncomingMessage = function(msg: TextChatMessage)
    if msg.TextSource then
        addListItem(chatLogs, "[" .. msg.TextSource.Name .. "]: " .. msg.Text)
    end
end

