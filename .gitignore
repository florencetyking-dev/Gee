-- SLIDER
local sliderH = Instance.new("Frame", lock)
sliderH.Size, sliderH.Position, sliderH.ZIndex = UDim2.new(0.7, 0, 0.12, 0), UDim2.new(0.15, 0, 0.82, 0), 101
