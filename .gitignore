local sButton = Instance.new("TextButton", sliderH)
sButton.Size, sButton.ZIndex, sButton.BackgroundColor3 = UDim2.new(0.25, 0, 1, 0), 105, Color3.fromRGB(100, 100, 255)
sButton.Text = ""
Instance.new("UICorner", sButton)

local sliding = false
sButton.MouseButton1Down:Connect(function() sliding = true end)
UIS.InputEnded:Connect(function(i) 
    if i.UserInputType == Enum.UserInputType.MouseButton1 or i.UserInputType == Enum.UserInputType.Touch then 
        if sButton.Position.X.Scale < 0.7 then TS:Create(sButton, TweenInfo.new(0.3), {Position = UDim2.new(0, 0, 0, 0)}):Play() end
        sliding = false 
    end 
end)
UIS.InputChanged:Connect(function(i)
    if sliding and (i.UserInputType == Enum.UserInputType.MouseMovement or i.UserInputType == Enum.UserInputType.Touch) then
        local p = math.clamp((i.Position.X - sliderH.AbsolutePosition.X) / sliderH.AbsoluteSize.X, 0, 0.75)
        sButton.Position = UDim2.new(p, 0, 0, 0)
        if p >= 0.7 then
            sliding = false
            TS:Create(lock, TweenInfo.new(0.8, Enum.EasingStyle.Quint), {Position = UDim2.new(0, 0, -1.2, 0)}):Play()
            task.wait(0.8) lock.Visible = false
        end
    end
end)

