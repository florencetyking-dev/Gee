-- [ NOTIFICATION SYSTEM ]
local function notify(text)
    local nFrame = Instance.new("Frame", sg)
    nFrame.Size, nFrame.Position = UDim2.new(0, 220, 0, 45), UDim2.new(0.5, -110, 1, 10)
    nFrame.BackgroundColor3, nFrame.ZIndex = Color3.fromRGB(25, 25, 35), 500
    Instance.new("UICorner", nFrame).CornerRadius = UDim.new(0, 8)
    local ns = Instance.new("UIStroke", nFrame)
    ns.Color, ns.Thickness = Color3.fromRGB(100, 100, 255), 2

    local nText = Instance.new("TextLabel", nFrame)
    nText.Size, nText.Text, nText.BackgroundTransparency = UDim2.new(1,0,1,0), text, 1
    nText.TextColor3, nText.Font, nText.TextSize = Color3.new(1,1,1), Enum.Font.GothamMedium, 14

    TS:Create(nFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back), {Position = UDim2.new(0.5, -110, 0.85, 0)}):Play()
    task.delay(3, function()
        TS:Create(nFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back), {Position = UDim2.new(0.5, -110, 1, 10)}):Play()
        Debris:AddItem(nFrame, 0.6)
    end)
end

